@{
    ViewData["Title"] = "Patient--" + ViewData.Model.name;
    @model Doctor;
    var result = ViewData.Model.payload;
    List<Dictionary<string,object>> res = (List<Dictionary<string,object>>)result;

}
<form class="Back" asp-controller="Doctor" asp-action="Back">
    <input type="submit" value="Back" />
    <input asp-for ="name" input type="hidden" value="@ViewData.Model.name" />
    <input asp-for="id" input type="hidden" value="@ViewData.Model.id" />

</form>
<div class="text-center">
    <h1 class="display-4">
        @ViewData.Model.name.ToString()
    </h1>

    <div>
        @foreach(var dic in res){
                    <section>
                    @foreach (string ky in dic.Keys) { 
                    <p> 
                        <span>
                            @ky
                        </span>
                        :
                        <span>
                        @dic[@ky]
                        </span>
                    </p><br/>
                    }
                    </section><br/><br>

        }
        

    </div>
</div>
